@page "/stats"
@page "/stats/winrate"
@using dsstats.shared
@using dsstats.razorlib.Stats.Winrate

<WinrateInComponent Request="Request" />

@code {
    WinrateRequest Request = new()
    {
        TimePeriod = TimePeriod.Past90Days,
        RatingType = RatingType.Cmdr,
        WinrateType = WinrateType.AvgGain,
        ComboRating = false,
        MauiPlayers = true
    };
}
